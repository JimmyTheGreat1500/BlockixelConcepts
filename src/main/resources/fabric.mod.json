{
  "schemaVersion": 1,
  "id": "blockixel",
  "version": "${version}",

  "name": "Blockixel Concepts",
  "description": "A Fabric mod that aims to implement Blockixel Artistry on YouTube's unique minecraft concepts.",
  "authors": [
    "JimmyTheGreat100"
  ],
  "contact": {
    "homepage": "totallynotagame.com",
    "sources": "https://github.com/JimmyTheGreat1500/blockixelconcepts"
  },

  "license": "CC0-1.0",
  "icon": "assets/blockixelconcepts/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.totallynotagame.blockixel.BlockixelConcepts"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
